{
  "name": "Etch",
  "version": "0.1.0",
  "summary": "Swift Static Site Framework.",
  "homepage": "https://github.com/kylef/Etch/",
  "license": { "type": "BSD", "file": "LICENSE" },
  "authors": { "Kyle Fuller": "kyle@fuller.li" },
  "social_media_url": "http://twitter.com/kylefuller",
  "source": { "git": "https://github.com/kylef/Etch.git", "tag": "0.1.0" },
  "platforms": {
    "osx": "10.7"
  },
  "dependencies": {
    "Stencil": [ "~> 0.4.0" ],
    "URITemplate": [ "~> 1.3" ],
    "PathKit": [],
    "Commander": [ "~> 0.3.0" ]
  },
  "source_files": ["Etch/*.swift", "Etch/{Reader,Processor,Generator}s/*.swift"],
  "entry_points": {
    "cli": {
      "etch": "bin/etch.swift"
    }
